{
  "name": "app",
  "version": "1.0.0",
  "main": "bootstrap.ts",
  "author": "Thao Phung",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "gen:module": "bystro module-template ./src",
    "prepare": "husky install",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed  && dotenv -e .env.test -- jest --verbose --runInBand --config ./e2e/jest.e2e-config.ts",
    "test:watch": "jest --watch --verbose",
    "start:dev": "ts-node-dev --respawn --transpile-only --no-notify --exit-child -r tsconfig-paths/register src/bootstrap.ts",
    "start:prod": "node dist/main"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "2.30.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "inversify": "^6.0.1",
    "jest-mock-extended": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "ts-guard-clause": "^0.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": ">=12",
    "prettier": "^2.7.1",
    "prisma": "2.30.3",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write",
      "yarn test --watchAll=false --findRelatedTests --bail"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
